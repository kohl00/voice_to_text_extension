{
    "manifest_version": 3,
    "name": "Speech to Text",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon128.png"
      }
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
      },
    "icons": {
      "128": "icon128.png"
    }
  }
  